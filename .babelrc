{
  "presets": [
    "react",
    "es2015",
    "es2016",
    "es2017",
    "stage-0"
  ],
  "plugins": [
    "react-html-attrs",
    "transform-decorators-legacy",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-class-properties",
    "transform-react-inline-elements",
    "transform-react-constant-elements",
    "transform-decorators-legacy",
    "syntax-dynamic-import"
  ]
}
